msgid ""
"\n"
"\t\t\t\t\tТак же обожал, в недавнем времени, \t\n"
"\t\t\t\t\t"
msgstr ""
"\n"
"\t\t\t\tI also loved, recently, \t\n"
"\t\t\t\t"

